{
  "name": "StartKIT",
  "private": true,
  "type": "module",
  "description": "Стартовая сборка для начала проекта",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^10.1.0",
    "browser-sync": "^2.26.13",
    "htmlhint": "^0.14.2",
    "eslint": "^7.15.0",
    "eslint-config-htmlacademy": "^0.6.0",
    "stylelint": "^13.8.0",
    "stylelint-config-htmlacademy": "^0.1.4",
    "css-mqpacker": "^7.0.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^4.0.1",
    "gh-pages": "^3.1.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-posthtml": "^3.0.5",
    "posthtml-include": "^1.6.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^3.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "test": "npm run editorconfig && npm run stylelint",
    "stylelint": "stylelint 'sass/**/*.scss' --syntax scss",
    "build": "gulp build",
    "start": "npm run build && gulp serve",
    "deploy": "gulp deploy"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "src/**/*.html",
    "*.json",
    "*.js",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/scss/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "^15.0.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
